<body class="app app-signup p-0">    	
    <div class="row g-0 app-auth-wrapper">
	    <div class="col-12 col-md-7 col-lg-6 auth-main-col text-center p-5">
		    <div class="d-flex flex-column align-content-end">
			    <div class="app-auth-body mx-auto">	
				    <div class="app-auth-branding mb-4"><a class="app-logo" href="index.html"><img class="logo-icon mr-2" src="assets/images/app-logo.svg" alt="logo"></a></div>
					<h2 class="auth-heading text-center mb-4">Registro de Usuario</h2>					
	
					<div class="auth-form-container text-left mx-auto">
						<form id="signup_form" class="auth-form auth-signup-form" action="/sign_up" method="post"> <!-- MN01 -->
							<div class="email mb-3">
								<label class="sr-only" for="inputNombre">Nombre</label>
								<input id="inputNombre" name="inputNombre" type="text" class="form-control signup-name" placeholder="Nombre" required="required">
                            </div>
                            <div class="email mb-3">
								<label class="sr-only" for="inputApellido">Apellido</label>
								<input id="inputApellido" name="inputApellido" type="text" class="form-control signup-name" placeholder="Apellido" required="required">
							</div>
							<div class="email mb-3">
								<label class="sr-only" for="inputEmail">Correo Electrónico</label>
								<input id="inputEmail" name="inputEmail" type="email" class="form-control signup-email" placeholder="Correo Electrónico" required="required">
							</div>
							<div class="password mb-3">
								<label class="sr-only" for="inputPassword">Contraseña</label>
								<input id="inputPassword" name="inputPassword" type="password" class="form-control signup-password" placeholder="Contraseña" required="required">
                            </div>
                            <div class="password mb-3">
								<label class="sr-only" for="inputConfPass">Confirmar Contraseña</label>
								<input id="inputConfPass" name="inputConfPass" type="password" class="form-control signup-password" placeholder="Confirmar Contraseña" required="required">
							</div>
							<div class="extra mb-3">
								<div class="form-check">
									<input class="form-check-input" type="checkbox" value="" id="RememberPassword">
									<label class="form-check-label" for="RememberPassword">
									Estoy de acuerdo con los <a href="#" class="app-link">Terminos</a> y <a href="#" class="app-link">Poliza de Privacidad</a>.
									</label>
								</div>
							</div><!--//extra-->
							
							<div id="signup_form_alert" class="alert fade show hidden" role="alert"> <!-- //MN01 -->
								<div id="signup_form_alert_content"></div>
							</div>

							<div class="text-center"> 
								<button type="submit" id="submit_signup_button" class="btn app-btn-primary btn-block theme-btn mx-auto">Continuar</button>
							</div>
						</form><!--//auth-form-->
						
						<div class="auth-option text-center pt-5">Ya tienes cuenta? <a class="text-link" href="/login">Ingresar</a></div>
					</div><!--//auth-form-container-->	
					
					
				    
			    </div><!--//auth-body-->
		    
			    <footer class="app-auth-footer">
				    <div class="container text-center py-3">
		
				    </div>
			    </footer><!--//app-auth-footer-->	
		    </div><!--//flex-column-->   
	    </div><!--//auth-main-col-->
	    <div class="col-12 col-md-5 col-lg-6 h-100 auth-background-col">
		    <div class="auth-background-holder">			    
		    </div>
		    <div class="auth-background-mask"></div>

	    </div><!--//auth-background-col-->
    
    </div><!--//row-->



 

</body>
</html>